(define (problem depotprob4321) (:domain Depot)
(:objects
	depot0 depot1 - depot
	distributor0 distributor1 distributor2 - distributor
	truck0 truck1 - truck
	pallet0 pallet1 pallet2 pallet3 pallet4 pallet5 - pallet
	crate0 crate1 crate2 crate3 crate4 crate5 crate6 crate7 crate8 - crate
	hoist0 hoist1 hoist2 hoist3 hoist4 hoist5 hoist6 hoist7 - hoist)
(:init
	(at pallet0 depot0)
	(clear crate4)
	(at pallet1 depot1)
	(clear crate5)
	(at pallet2 distributor0)
	(clear pallet2)
	(at pallet3 distributor1)
	(clear crate3)
	(at pallet4 distributor2)
	(clear crate6)
	(at pallet5 depot0)
	(clear crate8)
	(at truck0 distributor0)
	(= (current_load truck0) 0)
	(= (load_limit truck0) 392)
	(at truck1 distributor1)
	(= (current_load truck1) 0)
	(= (load_limit truck1) 245)
	(at hoist0 depot0)
	(available hoist0)
	(at hoist1 depot1)
	(available hoist1)
	(at hoist2 distributor0)
	(available hoist2)
	(at hoist3 distributor1)
	(available hoist3)
	(at hoist4 distributor2)
	(available hoist4)
	(at hoist5 depot0)
	(available hoist5)
	(at hoist6 distributor1)
	(available hoist6)
	(at hoist7 distributor0)
	(available hoist7)
	(at crate0 depot0)
	(on crate0 pallet5)
	(= (weight crate0) 67)
	(at crate1 depot0)
	(on crate1 pallet0)
	(= (weight crate1) 69)
	(at crate2 distributor2)
	(on crate2 pallet4)
	(= (weight crate2) 39)
	(at crate3 distributor1)
	(on crate3 pallet3)
	(= (weight crate3) 81)
	(at crate4 depot0)
	(on crate4 crate1)
	(= (weight crate4) 87)
	(at crate5 depot1)
	(on crate5 pallet1)
	(= (weight crate5) 72)
	(at crate6 distributor2)
	(on crate6 crate2)
	(= (weight crate6) 1)
	(at crate7 depot0)
	(on crate7 crate0)
	(= (weight crate7) 84)
	(at crate8 depot0)
	(on crate8 crate7)
	(= (weight crate8) 3)
	(= (fuel-cost) 0)
)

(:goal (and
		(on crate0 pallet0)
		(on crate1 pallet1)
		(on crate2 pallet4)
		(on crate3 crate1)
		(on crate4 crate6)
		(on crate5 pallet5)
		(on crate6 crate8)
		(on crate7 crate4)
		(on crate8 crate5)
	)
)

(:metric minimize (total-time)))
