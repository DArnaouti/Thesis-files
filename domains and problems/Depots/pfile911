	(at pallet5 distributor1)
	(clear crate2)
	(at pallet6 depot0)
	(clear crate4)
	(at pallet7 depot1)
	(clear pallet7)
	(at truck0 distributor0)
	(= (current_load truck0) 0)
	(= (load_limit truck0) 398)
	(at truck1 distributor1)
	(= (current_load truck1) 0)
	(= (load_limit truck1) 245)
	(at truck2 depot1)
	(= (current_load truck2) 0)
	(= (load_limit truck2) 221)
	(at hoist0 depot0)
	(available hoist0)
	(at hoist1 depot1)
	(available hoist1)
	(at hoist2 distributor0)
	(available hoist2)
	(at hoist3 distributor1)
	(available hoist3)
	(at hoist4 distributor2)
	(available hoist4)
	(at hoist5 distributor2)
	(available hoist5)
	(at hoist6 depot1)
	(available hoist6)
	(at hoist7 depot1)
	(available hoist7)
	(at crate0 depot0)
	(on crate0 pallet6)
	(= (weight crate0) 69)
	(at crate1 depot0)
	(on crate1 crate0)
	(= (weight crate1) 62)
	(at crate2 distributor1)
	(on crate2 pallet5)
	(= (weight crate2) 8)
	(at crate3 depot0)
	(on crate3 pallet0)
	(= (weight crate3) 28)
	(at crate4 depot0)
	(on crate4 crate1)
	(= (weight crate4) 74)
	(at crate5 depot0)
	(on crate5 crate3)
	(= (weight crate5) 89)
	(at crate6 depot0)
	(on crate6 crate5)
	(= (weight crate6) 93)
	(at crate7 distributor2)
	(on crate7 pallet4)
	(= (weight crate7) 20)
	(at crate8 depot0)
	(on crate8 crate6)
	(= (weight crate8) 90)
	(= (fuel-cost) 0)
)

(:goal (and
		(on crate0 pallet2)
		(on crate2 crate0)
		(on crate3 pallet5)
		(on crate4 crate7)
		(on crate5 pallet6)
		(on crate6 pallet3)
		(on crate7 crate8)
