(define (problem depotprob4321) (:domain Depot)
(:objects
	depot0 depot1 depot2 - depot
	distributor0 distributor1 distributor2 - distributor
	truck0 truck1 truck2 - truck
	pallet0 pallet1 pallet2 pallet3 pallet4 pallet5 - pallet
	crate0 crate1 crate2 crate3 crate4 crate5 crate6 crate7 crate8 - crate
	hoist0 hoist1 hoist2 hoist3 hoist4 hoist5 hoist6 hoist7 hoist8 hoist9 - hoist)
(:init
	(at pallet0 depot0)
	(clear crate5)
	(at pallet1 depot1)
	(clear crate8)
	(at pallet2 depot2)
	(clear pallet2)
	(at pallet3 distributor0)
	(clear pallet3)
	(at pallet4 distributor1)
	(clear crate3)
	(at pallet5 distributor2)
	(clear crate7)
	(at truck0 distributor0)
	(= (current_load truck0) 0)
	(= (load_limit truck0) 468)
	(at truck1 depot0)
	(= (current_load truck1) 0)
	(= (load_limit truck1) 246)
	(at truck2 distributor0)
	(= (current_load truck2) 0)
	(= (load_limit truck2) 357)
	(at hoist0 depot0)
	(available hoist0)
	(at hoist1 depot1)
	(available hoist1)
	(at hoist2 depot2)
	(available hoist2)
	(at hoist3 distributor0)
	(available hoist3)
	(at hoist4 distributor1)
	(available hoist4)
	(at hoist5 distributor2)
	(available hoist5)
	(at hoist6 depot2)
	(available hoist6)
	(at hoist7 distributor1)
	(available hoist7)
	(at hoist8 distributor2)
	(available hoist8)
	(at hoist9 distributor1)
	(available hoist9)
	(at crate0 depot1)
	(on crate0 pallet1)
	(= (weight crate0) 12)
	(at crate1 distributor2)
	(on crate1 pallet5)
	(= (weight crate1) 59)
	(at crate2 depot0)
	(on crate2 pallet0)
	(= (weight crate2) 9)
	(at crate3 distributor1)
	(on crate3 pallet4)
	(= (weight crate3) 29)
	(at crate4 depot1)
	(on crate4 crate0)
	(= (weight crate4) 40)
	(at crate5 depot0)
	(on crate5 crate2)
	(= (weight crate5) 31)
	(at crate6 distributor2)
	(on crate6 crate1)
	(= (weight crate6) 31)
	(at crate7 distributor2)
	(on crate7 crate6)
	(= (weight crate7) 91)
	(at crate8 depot1)
	(on crate8 crate4)
	(= (weight crate8) 5)
	(= (fuel-cost) 0)
)

(:goal (and
		(on crate1 crate5)
		(on crate3 crate8)
		(on crate4 crate7)
		(on crate5 pallet4)
		(on crate6 pallet5)
		(on crate7 pallet2)
		(on crate8 pallet3)
	)
)

(:metric minimize (total-time)))
