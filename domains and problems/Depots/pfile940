	(= (weight crate6) 93)
	(at crate7 distributor3)
	(on crate7 crate5)
	(= (weight crate7) 66)
	(at crate8 distributor1)
	(on crate8 pallet4)
	(= (weight crate8) 39)
	(= (fuel-cost) 0)
)

(:goal (and
		(on crate1 pallet6)
		(on crate2 crate4)
		(on crate3 crate8)
		(on crate4 pallet5)
		(on crate5 pallet1)
		(on crate6 crate3)
		(on crate7 pallet3)
		(on crate8 pallet4)
	)
)

(:metric minimize (total-time)))
