(define (problem depotprob4321) (:domain Depot)
(:objects
	depot0 depot1 depot2 - depot
	distributor0 distributor1 distributor2 distributor3 - distributor
	truck0 truck1 truck2 - truck
	pallet0 pallet1 pallet2 pallet3 pallet4 pallet5 pallet6 pallet7 - pallet
	crate0 crate1 crate2 crate3 crate4 crate5 crate6 crate7 crate8 - crate
	hoist0 hoist1 hoist2 hoist3 hoist4 hoist5 hoist6 hoist7 hoist8 - hoist)
(:init
	(at pallet0 depot0)
	(clear crate1)
	(at pallet1 depot1)
	(clear pallet1)
	(at pallet2 depot2)
	(clear crate6)
	(at pallet3 distributor0)
	(clear pallet3)
	(at pallet4 distributor1)
	(clear crate8)
	(at pallet5 distributor2)
	(clear crate2)
	(at pallet6 distributor3)
	(clear crate7)
	(at pallet7 depot2)
	(clear crate4)
	(at truck0 depot0)
	(= (current_load truck0) 0)
	(= (load_limit truck0) 417)
	(at truck1 depot0)
	(= (current_load truck1) 0)
	(= (load_limit truck1) 333)
	(at truck2 distributor1)
	(= (current_load truck2) 0)
	(= (load_limit truck2) 215)
	(at hoist0 depot0)
	(available hoist0)
	(at hoist1 depot1)
	(available hoist1)
	(at hoist2 depot2)
	(available hoist2)
	(at hoist3 distributor0)
	(available hoist3)
	(at hoist4 distributor1)
	(available hoist4)
	(at hoist5 distributor2)
	(available hoist5)
	(at hoist6 distributor3)
	(available hoist6)
	(at hoist7 depot1)
	(available hoist7)
	(at hoist8 distributor3)
	(available hoist8)
	(at crate0 depot2)
	(on crate0 pallet7)
	(= (weight crate0) 98)
	(at crate1 depot0)
	(on crate1 pallet0)
	(= (weight crate1) 100)
	(at crate2 distributor2)
	(on crate2 pallet5)
	(= (weight crate2) 30)
	(at crate3 depot2)
	(on crate3 crate0)
	(= (weight crate3) 85)
	(at crate4 depot2)
	(on crate4 crate3)
	(= (weight crate4) 55)
	(at crate5 distributor3)
	(on crate5 pallet6)
	(= (weight crate5) 63)
	(at crate6 depot2)
