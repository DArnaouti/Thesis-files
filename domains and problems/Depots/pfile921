(define (problem depotprob4321) (:domain Depot)
(:objects
	depot0 depot1 - depot
	distributor0 distributor1 distributor2 - distributor
	truck0 truck1 truck2 - truck
	pallet0 pallet1 pallet2 pallet3 pallet4 - pallet
	crate0 crate1 crate2 crate3 crate4 crate5 crate6 crate7 crate8 - crate
	hoist0 hoist1 hoist2 hoist3 hoist4 hoist5 hoist6 hoist7 hoist8 hoist9 - hoist)
(:init
	(at pallet0 depot0)
	(clear crate8)
	(at pallet1 depot1)
	(clear crate2)
	(at pallet2 distributor0)
	(clear pallet2)
	(at pallet3 distributor1)
	(clear crate3)
	(at pallet4 distributor2)
	(clear crate7)
	(at truck0 distributor0)
	(= (current_load truck0) 0)
	(= (load_limit truck0) 392)
	(at truck1 distributor1)
	(= (current_load truck1) 0)
	(= (load_limit truck1) 245)
	(at truck2 depot1)
	(= (current_load truck2) 0)
	(= (load_limit truck2) 221)
	(at hoist0 depot0)
	(available hoist0)
	(at hoist1 depot1)
	(available hoist1)
	(at hoist2 distributor0)
	(available hoist2)
	(at hoist3 distributor1)
	(available hoist3)
	(at hoist4 distributor2)
	(available hoist4)
	(at hoist5 distributor2)
	(available hoist5)
	(at hoist6 depot1)
	(available hoist6)
	(at hoist7 depot1)
	(available hoist7)
	(at hoist8 distributor0)
	(available hoist8)
	(at hoist9 depot0)
	(available hoist9)
	(at crate0 distributor1)
	(on crate0 pallet3)
	(= (weight crate0) 81)
	(at crate1 depot0)
	(on crate1 pallet0)
	(= (weight crate1) 87)
	(at crate2 depot1)
	(on crate2 pallet1)
	(= (weight crate2) 72)
	(at crate3 distributor1)
	(on crate3 crate0)
	(= (weight crate3) 1)
	(at crate4 distributor2)
	(on crate4 pallet4)
	(= (weight crate4) 84)
	(at crate5 distributor2)
	(on crate5 crate4)
	(= (weight crate5) 3)
	(at crate6 depot0)
	(on crate6 crate1)
	(= (weight crate6) 12)
	(at crate7 distributor2)
	(on crate7 crate5)
	(= (weight crate7) 59)
	(at crate8 depot0)
	(on crate8 crate6)
	(= (weight crate8) 9)
	(= (fuel-cost) 0)
)

(:goal (and
		(on crate0 crate3)
		(on crate2 pallet3)
		(on crate3 pallet1)
		(on crate4 crate6)
		(on crate5 crate4)
		(on crate6 crate8)
		(on crate7 crate0)
		(on crate8 pallet4)
	)
)

(:metric minimize (total-time)))
